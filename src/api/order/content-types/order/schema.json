{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "description": {
      "type": "string"
    },
    "order_status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "done"
      ],
      "default": "pending",
      "required": true,
      "enumName": "pending"
    },
    "merchant": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::merchant.merchant",
      "inversedBy": "orders"
    }
  }
}
